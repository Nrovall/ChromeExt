{
  "manifest_version": 2,

  "name": "ConnectWin",
  "description": "This extension allows you to quickly search for HW answers to Connect problems",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "I Hate HW"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
